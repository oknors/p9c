{
  "kind": "collectionType",
  "collectionName": "links",
  "info": {
    "name": "link",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "desc": {
      "type": "richtext"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Analytics",
        "Exchanges",
        "Pools"
      ],
      "default": "Analytics"
    },
    "logo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "url": {
      "type": "string",
      "required": true
    }
  }
}
